{
  "name": "react-ssr-starting-point",
  "version": "1.0.0",
  "description": "React SSR starting point with JSX, Babel and Webpack",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/config/webpack.prod.js",
    "build:server": "cross-env NODE_ENV=production babel src/server --out-dir dist/server --extensions \".js\" --copy-files",
    "clean": "rimraf dist build",
    "dev": "concurrently \"cross-env NODE_ENV=development nodemon --exec babel-node src/server/server.js\" \"webpack serve --config webpack/config/webpack.dev.js\"",
    "start": "cross-env NODE_ENV=production node dist/server/server.js"
  },
  "keywords": [
    "react",
    "ssr",
    "jsx",
    "babel",
    "webpack"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "react-refresh": "^0.14.0",
    "rimraf": "^6.0.1",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  }
}
