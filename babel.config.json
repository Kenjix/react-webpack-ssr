{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "18"
        }
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic",
        "development": false
      }
    ]
  ],
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "18"
            }
          }
        ],
        [
          "@babel/preset-react",
          {
            "runtime": "automatic",
            "development": true
          }
        ]
      ],
      "plugins": ["react-refresh/babel"]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "18"
            }
          }
        ],
        [
          "@babel/preset-react",
          {
            "runtime": "automatic",
            "development": false
          }
        ]
      ],
      "plugins": []
    }
  }
}